{
  "manifest_version": 2,

  "name": "WeScribe",
  "description": "This extension tracks user usage.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides" : {
    "newtab": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}
